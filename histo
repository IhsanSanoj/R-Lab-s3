# Student heights and frequencies
heights <- c(137.5, 142.5, 147.5, 152.5)  # Midpoints of the intervals
students <- c(4, 12, 16, 8)

# Plotting the histogram using barplot since raw data isn't given
barplot(students,
        names.arg = c("135-140", "140-145", "145-150", "150-155"),
        xlab = "Height Range (cm)",
        ylab = "Number of Students",
        main = "Histogram of Student Heights",
        col = "skyblue")