# Population data for states
population <- c(4779736, 710231, 6392017, 2915918, 37253956, 5029196, 3574097, 897934)

# Calculate statistics
mean_pop <- mean(population)
median_pop <- median(population)
variance_pop <- var(population)

# Display results
cat("Mean Population:", mean_pop, "\n")
cat("Median Population:", median_pop, "\n")
cat("Variance of Population:", variance_pop, "\n")